{
  "name": "hxt",
  "version": "1.0.0",
  "description": "Обучение на hexlet",
  "main": "main.js",
  "dependencies": {
    "eslint-config-airbnb-base": "^12.0.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint": "^4.8.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "hexlet-pairs-data": "^1.0.6",
    "i": "^0.3.6",
    "npm": "^5.5.1",
    "onchange": "^3.2.1",
    "parallelshell": "^3.0.2"
  },
  "scripts": {
    "lint-f": "eslint main.js --fix",
    "babel-build": "babel src -d dist --presets es2015",
    "babel-watch": "babel src -d dist --presets es2015 -w",
    "start-main_js": "node dist/main.js",
    "rjs": "onchange 'dist/*.js' -- npm run start-main_js",
    "wjs": "parallelshell 'npm run babel-watch' 'npm run rjs'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nik4un/hxt.git"
